/**/
'PIPE (end ?)',
     'cp Q TERM',
     '| split at ,',
     '| strip',
     '| pick w1.1 == ~MORE~ or',
            'w1.1 == ~HOLD~',
     '| join * x40',
     '| if: if pick w2.1 == ~010~',
     '|   change ~010~000~',
     '|   change ~ON~OFF~',
     '| if:',
     '|   change ~000~010~',
     '|   change ~OFF~ON~',
     '| if:',
     '| specs ~TERM~ 1 w1;* nw',
     '| cons',
     '| cp',
     '| cons'
