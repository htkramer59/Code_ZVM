<dir>                                                                           
/etc/sysconfig/network/                                                         
</dir>                                                                          
                                                                                
<file>                                                                          
ifcfg-eth0.VLAN                                                                 
</file>                                                                         
                                                                                
<content>                                                                       
BOOTPROTO='static'                                                              
STARTMODE='auto'                                                                
ETHERDEVICE='eth0'                                                              
IPADDR='HOST_IP/NET_MASK'                                                       
</content>                                                                      
                                                                                
<file>                                                                          
ifcfg-eth0                                                                      
</file>                                                                         
                                                                                
<content>                                                                       
#BOOTPROTO='static'                                                             
#STARTMODE='auto'                                                               
#IPADDR='HOST_IP/NET_SIZE'                                                      
</content>                                                                      
###                                                                             
<file>                                                                          
ifroute-eth0                                                                    
</file>                                                                         
                                                                                
<content>                                                                       
default GATE_WAY - eth0                                                         
</content>                                                                      
